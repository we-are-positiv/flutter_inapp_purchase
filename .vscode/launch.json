{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Start Docusaurus",
      "cwd": "${workspaceFolder}/docs",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "start"],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "env": {
        "NODE_ENV": "development"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Build Docusaurus",
      "cwd": "${workspaceFolder}/docs",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "build"],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Serve Docusaurus Build",
      "cwd": "${workspaceFolder}/docs",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "serve"],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "name": "Flutter: Attach to Device",
      "type": "dart",
      "request": "attach"
    },
    {
      "name": "Flutter: Run Example App",
      "type": "dart",
      "request": "launch",
      "program": "${workspaceFolder}/example/lib/main.dart"
    },
    {
      "name": "Flutter: Run Example App (Profile Mode)",
      "type": "dart",
      "request": "launch",
      "program": "${workspaceFolder}/example/lib/main.dart",
      "flutterMode": "profile"
    },
    {
      "name": "Flutter: Run Example App (Release Mode)",
      "type": "dart",
      "request": "launch",
      "program": "${workspaceFolder}/example/lib/main.dart",
      "flutterMode": "release"
    }
  ],
  "compounds": []
}